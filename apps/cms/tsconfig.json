{
  "compilerOptions": {
    "target": "ES2021",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": false,
    "baseUrl": "./",
    "rootDir": "./",
    "types": ["node"],
    "resolveJsonModule": true,
    "noEmit": true
  },
  "include": ["./**/*.ts", "./scripts/**/*.js"],
  "files": ["./scripts/seed.js"],
  "exclude": ["node_modules", "build", "dist"]
}
